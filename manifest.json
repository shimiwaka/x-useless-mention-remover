{
  "manifest_version": 3,
  "name": "X Useless Mention Remover",
  "version": "1.0",
  "description": "Removes 'via' parameter from X (Twitter) share URLs",
  "permissions": ["tabs"],
  "host_permissions": [
    "https://twitter.com/*",
    "https://x.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
